{
  "name": "ghactions-service-update",
  "version": "0.3.3",
  "private": true,
  "description": "Update one or more Services running in Duplo",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "ci-commit": "npm run build && npm run format && npm run lint && npm run package && npm test --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duplocloud/ghactions-service-update.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "axios": "^1.6.1",
    "axios-observable": "^2.0.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^16.10.5",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/parser": "^5.13.0",
    "@vercel/ncc": "^0.33.3",
    "babel-jest": "^29.2.1",
    "eslint": "^8.26.0",
    "eslint-plugin-github": "^4.4.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.1.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.4.4"
  }
}
