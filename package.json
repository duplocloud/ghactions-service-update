{
  "name": "ghactions-service-update",
  "version": "0.2.4",
  "private": true,
  "description": "Update one or more Services running in Duplo",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "ci-commit": "npm run build && npm run format && npm run lint && npm run package && npm test --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duplocloud/ghactions-service-update.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "node-fetch": "^2.6.7",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^18.6.2",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/parser": "^5.13.0",
    "@vercel/ncc": "^0.33.3",
    "babel-jest": "^27.5.1",
    "eslint": "^8.10.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.4.4"
  }
}
